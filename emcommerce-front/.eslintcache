ubmit} className=\"btn btn-primary\">\r\n                Submit\r\n            </button>\r\n        </form>\r\n\r\n\r\n\r\n    )\r\n\r\n     const handleChange = name => event => {\r\n\r\n\r\n        setValues({...values,error:false, [name]:event.target.value})\r\n\r\n\r\n\r\n\r\n\r\n\r\n     }\r\n\r\n     const showError = ()=>(\r\n         <div className=\"alert alert-danger\" style={{display:error ? '':'none'}}>\r\n             {error}\r\n         </div>\r\n     )\r\n\r\n\r\n     const showLoading = () => (\r\n        loading && (<div className=\"alert alert-info\">\r\n            <h2>Loading...</h2>\r\n        </div>)\r\n\r\n     )\r\n\r\n     const redirectUser = () =>{\r\n\r\n        if(redirectToReferrer){\r\n            if(user && user.role === 1){\r\n                return <Redirect to=\"/admin/dashboard\"/>\r\n\r\n            }\r\n            else{\r\n                return <Redirect to=\"/user/dashboard\"/>\r\n            }\r\n        }\r\n\r\n        // if( redirectToReferrer\r\n        //     ){\r\n\r\n        //     return<Redirect to='/'/>;\r\n        }\r\n     \r\n\r\n    return(\r\n    <Layout title=\"Sign in\" description=\"Sign up to the App\"\r\n     className=\"container col-md-8 offset-md-2\">\r\n        \r\n        {showError()}\r\n        {showLoading()}\r\n        {signUpForm()}\r\n        {redirectUser()}\r\n\r\n\r\n        {/* {JSON.stringify(values)} */}\r\n\r\n     </Layout>\r\n\r\n    )\r\n\r\n    }\r\n\r\n\r\n\r\n\r\n\r\nexport  default Signin","C:\\Users\\nbatr\\OneDrive\\ecommerce2\\emcommerce-front\\src\\core\\Home.js",[],"C:\\Users\\nbatr\\OneDrive\\ecommerce2\\emcommerce-front\\src\\core\\Menu.js",["71"],"C:\\Users\\nbatr\\OneDrive\\ecommerce2\\emcommerce-front\\src\\core\\Layout.js",[],"C:\\Users\\nbatr\\OneDrive\\ecommerce2\\emcommerce-front\\src\\config.js",[],"C:\\Users\\nbatr\\OneDrive\\ecommerce2\\emcommerce-front\\src\\auth\\apiIndex.js",[],"C:\\Users\\nbatr\\OneDrive\\ecommerce2\\emcommerce-front\\src\\user\\Userdashboard.